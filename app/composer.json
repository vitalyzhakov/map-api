{
    "name": "vitalyzhakov/map-api",
    "description": "API for OpenStreetMap",
    "homepage": "https://vnytve.ru/",
    "type": "project",
    "license": "BSD-3-Clause",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1.0",
        "ext-intl": "*",
        "yiisoft/yii2": "*",
        "fxp/composer-asset-plugin": "*",
        "e96/yii2-sentry": "*",
        "webonyx/graphql-php": "^0.11.5"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "*",
        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1",
        "codeception/specify": "~0.4.3"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
